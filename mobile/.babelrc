{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "components": "./src/components",
              "constants": "./src/constants",
              "stores": "./src/stores",
              "navigation": "./src/navigation",
              "utilities": "./src/utilities"
            }
          }
        ]
      ]
    }
  }
}
