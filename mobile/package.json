{
  "main": "src/App.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start-dev": "yarn env-dev && expo start",
    "start-stg": "yarn env-stg && expo start",
    "start-prd": "yarn env-prd && expo start",
    "publish-dev": "yarn env-dev && expo publish --release-channel dev",
    "publish-stg": "yarn env-stg && expo publish --release-channel stg",
    "publish-prd": "cli-confirm 'Publish to PRODUCTION? (y/n)' && yarn env-prd && expo publish --release-channel prd",
    "env-dev": "cp ./__environment__/.dev.env .env",
    "env-stg": "cp ./__environment__/.stg.env .env",
    "env-prd": "cp ./__environment__/.prd.env .env"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.1",
    "buffer": "^5.1.0",
    "color": "^3.0.0",
    "expo": "^29.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "pubsub-js": "^1.6.0",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-apollo": "^2.2.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-native-animatable": "^1.2.4",
    "react-native-gesture-handler": "^1.0.0-alpha.43",
    "react-native-paint": "^1.1.0",
    "react-native-paper": "^2.2.4",
    "react-native-tab-view": "^0.0.77",
    "react-navigation": "^2.0.1",
    "react-navigation-material-bottom-tabs": "^0.4.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-flow": "^6.23.0",
    "cli-confirm": "^1.0.0",
    "flow-bin": "^0.76.0",
    "react-native-dotenv": "^0.1.1",
    "reactotron-react-native": "^2.0.0"
  },
  "moduleRoots": [
    "./"
  ]
}
