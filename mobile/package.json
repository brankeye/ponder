{
  "main": "src/App.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start-dev": "yarn env-dev && expo start",
    "start-prd": "yarn env-prd && expo start",
    "publish-default": "yarn env-dev && expo publish",
    "publish-dev": "yarn env-dev && expo publish --release-channel dev",
    "publish-prd": "cli-confirm 'Publish to PRODUCTION? (y/n)' && yarn env-prd && expo publish --release-channel prd",
    "env-dev": "cp ./__environment__/.dev.env .env",
    "env-prd": "cp ./__environment__/.prd.env .env"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.1",
    "buffer": "^5.1.0",
    "color": "^3.0.0",
    "date-fns": "^1.29.0",
    "expo": "^32.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "pubsub-js": "^1.6.0",
    "ramda": "^0.26.0",
    "react": "16.5.0",
    "react-apollo": "^2.2.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-animatable": "^1.2.4",
    "react-native-paint": "^1.1.0",
    "react-native-paper": "2.2.2",
    "react-navigation": "^3.0.9",
    "react-navigation-tabs": "1.0.0-alpha.4",
    "recompose": "^0.30.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@playlyfe/gql": "^2.6.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-expo": "^5.1.1",
    "cli-confirm": "^1.0.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.76.0",
    "node-fetch": "^2.3.0",
    "react-native-dotenv": "^0.2.0",
    "reactotron-react-native": "^2.0.0"
  },
  "moduleRoots": [
    "./"
  ]
}
